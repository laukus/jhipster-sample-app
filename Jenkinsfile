pipeline {
  agent any
  stages {
    stage('Build Backend') {
      steps {
        echo 'Hello'
      }
    }
    stage('Test Backend') {
      steps {
        echo 'Hello from Test Backend'
      }
    }
    stage('More Tests') {
      steps {
        echo 'More Tests'
      }
    }
    stage('Deploy Staging') {
      steps {
        echo 'Deploy Stage'
      }
    }
    stage('Deploy Production') {
      steps {
        echo 'Deploy Prod'
      }
    }
  }
}